[android]

modules="org/godotengine/godot/GodotAdMob"

[application]

config/name="Dodge"
run/main_scene="res://Main/Main.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://Globals/Globals.gd"

[display]

