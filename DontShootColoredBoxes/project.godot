; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="DontShootColoredBoxes"
run/main_scene="res://Game/Game.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://Globals.gd"

[display]

window/size/width=500
window/size/height=900
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="no_ball"
2d_physics/layer_2="red ball"
2d_physics/layer_3="red box"
2d_physics/layer_4="green ball"
2d_physics/layer_5="green box"
2d_physics/layer_6="blue ball"
2d_physics/layer_7="blue box"

[rendering]

environment/default_environment="res://default_env.tres"
